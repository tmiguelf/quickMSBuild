<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<DeploymentDirectory>C:\MyPlatform_Remote\Bin\</DeploymentDirectory>
		<DebuggerFlavor>WindowsRemoteDebugger</DebuggerFlavor>
		<RemoteDebuggerServerName>192.168.1.1</RemoteDebuggerServerName>
		<RemoteDebuggerWorkingDirectory>$(DeploymentDirectory)</RemoteDebuggerWorkingDirectory>
		<RemoteDebuggerCommand>$(DeploymentDirectory)MyApp.exe</RemoteDebuggerCommand>
	</PropertyGroup>
</Project>