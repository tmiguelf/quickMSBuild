<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup Label="ProjectConfigurations">
    <ProjectConfiguration Include="Debug|x64">
      <Configuration>Debug</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Release|x64">
      <Configuration>Release</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="WSL_Debug|x64">
      <Configuration>WSL_Debug</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="WSL_Release|x64">
      <Configuration>WSL_Release</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
  </ItemGroup>
  <PropertyGroup Label="Globals">
    <VCProjectVersion>16.0</VCProjectVersion>
    <ProjectGuid>{$guid1$}</ProjectGuid>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <BuildType>Debug</BuildType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <BuildType>Release</BuildType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='WSL_Debug'">
    <BuildType>Debug</BuildType>
    <BuildMethod>wsl</BuildMethod>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='WSL_Release'">
    <BuildType>Release</BuildType>
    <BuildMethod>wsl</BuildMethod>
  </PropertyGroup>
  <PropertyGroup Label="Configuration">
    <ConfigurationType>DynamicLibrary</ConfigurationType>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" />
  <ImportGroup Label="PropertySheets">
    <Import Project="$(SolutionDir)locations.props" />
    <Import Project="$(quickMSBuildPath)default.cpp.props" />
    <Import Project="$(versionLibPath)versionLib.include.props" />
  </ImportGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_$safeprojectname$_EXPORTS_;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>$(ProjectDir)include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <ClCompile Include="src\$projectname$.cpp" />
  </ItemGroup>
  <ItemGroup>
    <ClInclude Include="include\$projectname$\$projectname$.hpp" />
    <ClInclude Include="include\$projectname$\$projectname$_api.h" />
    <ClInclude Include="resources\versionSpecific.h" />
  </ItemGroup>
  <ItemGroup>
    <ResourceCompile Include="resources\dllVersion.rc" />
  </ItemGroup>
  <ItemGroup>
    <None Include="$projectname$.import.props" />
    <None Include="$projectname$.include.props" />
  </ItemGroup>
  <Import Project="$(quickMSBuildPath)default.cpp.targets" />
</Project>